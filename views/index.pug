extends layout

block content
	#modal1.modal.modal-fixed-footer
		.modal-content
			h4 Modal Header
			p A bunch of text
		.modal-footer
			a.modal-action.modal-close.waves-effect.waves-green.btn-flat(href='#!') Close
	.parallax-container.one
		.container
			.row.center-align
				//- h1 Hello World!
				h3.message #{message} 
		.parallax
			img(src="images/code1.jpeg")
	.section#about
		.row.container
			.col.s8
				h3 A Little About Me
				p My name is Jason Li and I am a Full Stack Software Engineer. I graduated with a finance degree at Georgia State University. Upon graduation I found myself entering a payroll company where I served as a consultant. After using MySQL, I got super interested in programming and how things worked behind the scenes. I did my research and used my online resources, but later decided that it wasn't enough and want to pursue a master's degree. I'm currently enrolled at Kennesaw State University for their Master's in Software Engineering. 
			.col.s4
				img(src="images/profileImage.JPG")
	.parallax-container.two
		.parallax
			img(src="images/code3.jpeg")
	.section
		.container
				.row
					.col.s8
						h1(style="color:#36A5E0;") Projects
					.col.s4
						a.modal-trigger(href="#modal1")
							h4 Click For More
				.row
					.col.s6
						a(href="https://petbasket.jyl.io/")
							span(style="font-size:30px") PetBasket | 
						a(href="https://github.com/2monsta/petBasket")
							span(style="font-size:30px") Github
						video(src="images/petBasket.mov" width="100%" height="300px" loop=true autoplay=true)
					.col.s6
						a(href="https://blackjack.jyl.io/")
							span(style="font-size:30px") BlackJack | 
						a(href="https://github.com/2monsta/blackJack2")
							span(style="font-size:30px") Github
						video(src="images/blackjack.mov" width="100%" height="300px" loop=true autoplay=true)
		.row.container#skills
			.col.s6.center-align
				h4 What I know
				.row.center-align
					.col.s3
						img(src="images/nodejs.png" width="100%")
					.col.s3
						img(src="images/express.png" width="100%")
					.col.s3
						img(src="images/react.png" width="100%")
					.col.s3
						img(src="images/redux.png" width="50%")
				.row.center-align
					.col.s3
						img(src="images/mysql.png" width="100%")
					.col.s3
						img(src="images/sass.png" width="100%")
					.col.s3
						img(src="images/mysql.png" width="100%")
					.col.s3
						img(src="images/python.png" width="100%")
			.col.s6.center-align
				h4 What I'm Learning
				.row.center-align
					.col.s3
						img(src="images/java.png" width="50%")
					.col.s3
						img(src="images/swift.png" width="50%")
					.col.s3
						img(src="images/golang.png" width="50%")
				.row.center-align
					.col.s3
						img(src="images/spring.png" width="100%" style="margin-left: 15px;")
			

			